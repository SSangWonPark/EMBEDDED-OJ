#include <stdio.h>
 
void AddBlank(unsigned char input[], unsigned char output[]) {
 
    int i = 0;
    int k = 0;
 
    while (input[i] != '\0')
    {
        output[i + k] = input[i];
 
        i++;
 
        if (i % 3 == 0)
        {
            output[i + k] = ' ';
            k++;
        }
    }
 
    output[i + k] = '\0';
};
 
int main() {
 
    unsigned char input[100], output[300];
 
    scanf("%s", input);
 
    AddBlank(input, output);
 
    printf("%s", output);
 
    return 0;
}
