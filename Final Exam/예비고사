#if 0
#include <stdio.h>

void esestrcpy(char result[], char arr[]) {
    int i = 0;
    int k = 0;
    while (arr[i] != '\0')
    {
        if (arr[i] >= 'A' && arr[i] <= 'Z')
        {
            result[k] = arr[i];
            k++;
        }
        i++;
    }
    result[k] = '\0';
}


int main(void)
{
    char arr[100];
    char result[100];

    scanf("%s", arr);

    esestrcpy(result, arr);

    printf("%s", result);
    return 0;
}
#endif

#if 0
#include <stdio.h>
#include <stdlib.h>

int main() {

    int n = 0;
    int max, min;
    int* p;

    scanf("%d", &n);

    p = (int*)malloc(sizeof(int) * n);

    for (int i = 0; i < n; i++)
    {
        scanf("%d", &p[i]);
    }

    max = p[0];
    min = p[0];

    for (int i = 0; i < n; i++)
    {
        max = max > p[i] ? max : p[i];
        min = min < p[i] ? min : p[i];
    }

    printf("%d %d", max, min);

    free(p);
}
