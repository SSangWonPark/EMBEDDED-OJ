#if 0
#include <stdio.h>

struct human {
    char name[10];
    int score;
};

int main() {

    struct human persen[10];
    struct human temp;
    int n = 0;

    scanf("%d", &n);

    for (int i = 0; i < n; i++)
    {
        scanf("%s %d", &persen[i].name, &persen[i].score);
    }

    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < n - 1; j++)
        {
            if (persen[j].score < persen[j+1].score)
            {
                temp = persen[j];
                persen[j] = persen[j + 1];
                persen[j + 1] = temp;
            }
        }
    }
    
    for (int i = 0; i < n; i++)
    {
        printf("%s %d\n", persen[i].name, persen[i].score);
    }

    return 0;
}
